@using System.Data;

@{ 
    DataSet dsCharts = SurveyApp.DataHelper.DashboardGetCharts();
    string dASP = "", pASP = "", tASP = "";
    string dABC = "", pABC = "", tABC = "", pSRS = "", tSRS = "";
    string dPEDsql = "", pPEDsql = "";
    string dCareGiver = "", pCareGiver = "";
    string dPES = "", pPES = "";
    string dGSI = "", pGSI = "";
    string dAPRS = "", tAPRS = "";
    string dWT = "", tWT = "";

    string dpABCg = "", aspABCg = "", tstpABCg = "", unpABCg = "", vspABCg = "", qpABCg = "";
    string dtABCg = "", astABCg = "", tsttABCg = "", untABCg = "", vstABCg = "", qtABCg = "";

    string dpSRSg = "", aspSRSg = "", tstpSRSg = "", unpSRSg = "", vspSRSg = "", qpSRSg = "";
    string dtSRSg = "", astSRSg = "", tsttSRSg = "", untSRSg = "", vstSRSg = "", qtSRSg = "";

    string dpPQLg = "", aspPQLg = "", tstpPQLg = "";
    string dpWLCg = "", aspWLCg = "", tstpWLCg = "";

    //Auditory Sensitivity Taste/Smell Sensitivity  Underresponsiveness Visual Sensitivity  Questionnaire

    if (dsCharts != null && dsCharts.Tables != null && dsCharts.Tables.Count > 0 && dsCharts.Tables[0].Rows.Count > 0)
    {
        foreach (DataRow dr in dsCharts.Tables[0].Rows)
        {
            #region Abbreviated Sensory Profile
            if (dr["SurveyID"] != DBNull.Value && (int)dr["SurveyID"] == 1)
            {
                dASP += "'" + (dr["ScheduleDate"] != DBNull.Value ? Convert.ToDateTime(dr["ScheduleDate"]).ToString("MM/dd/yyyy") : "") + "',";
                if(dr["UserType"] != DBNull.Value && (int)dr["UserType"] == 1)
                {
                    pASP += dr["Score"] + ",";
                    tASP += "0" + ",";
                }
                else if (dr["UserType"] != DBNull.Value && (int)dr["UserType"] == 2)
                {
                    tASP += dr["Score"] + ",";
                    pASP += "0" + ",";
                }
            }
            #endregion

            #region Aberrant Behavior Checklist Social Responsiveness Scale
            if (dr["SurveyID"] != DBNull.Value && ((int)dr["SurveyID"] == 4 || (int)dr["SurveyID"] == 5))
            {
                dABC += "'" + (dr["ScheduleDate"] != DBNull.Value ? Convert.ToDateTime(dr["ScheduleDate"]).ToString("MM/dd/yyyy") : "") + "',";

                if (dr["UserType"] != DBNull.Value && (int)dr["UserType"] == 1)
                {
                    if (dr["SurveyID"] != DBNull.Value && (int)dr["SurveyID"] == 4)
                    {
                        pABC += dr["Score"] + ",";
                        tABC += "0" + ",";
                        pSRS += "0" + ",";
                        tSRS += "0" + ",";
                    }
                    else if(dr["SurveyID"] != DBNull.Value && (int)dr["SurveyID"] == 5)
                    {
                        pSRS += dr["Score"] + ",";
                        tSRS += "0" + ",";
                        pABC += "0" + ",";
                        tABC += "0" + ",";
                    }
                }
                else if (dr["UserType"] != DBNull.Value && (int)dr["UserType"] == 2)
                {
                    if ((int)dr["SurveyID"] == 4)
                    {
                        tABC += dr["Score"] + ",";
                        pABC += "0" + ",";
                        pSRS += "0" + ",";
                        tSRS += "0" + ",";
                    }
                    else if((int)dr["SurveyID"] == 5)
                    {
                        tSRS += dr["Score"] + ",";
                        pSRS += "0" + ",";
                        pABC += "0" + ",";
                        tABC += "0" + ",";
                    }
                }
            }
            #endregion

            #region PedsQL-Parent Report for Children(8-12)
            if (dr["SurveyID"] != DBNull.Value && (int)dr["SurveyID"] == 8)
            {
                if (dr["UserType"] != DBNull.Value && (int)dr["UserType"] == 1)
                {
                    dPEDsql += "'" + (dr["ScheduleDate"] != DBNull.Value ? Convert.ToDateTime(dr["ScheduleDate"]).ToString("MM/dd/yyyy") : "") + "',";
                    pPEDsql += dr["Score"] + ",";
                }
            }
            #endregion

            #region Caregiver Strain Questionnaire
            if (dr["SurveyID"] != DBNull.Value && (int)dr["SurveyID"] == 2)
            {
                if (dr["UserType"] != DBNull.Value && (int)dr["UserType"] == 1)
                {
                    dCareGiver += "'" + (dr["ScheduleDate"] != DBNull.Value ? Convert.ToDateTime(dr["ScheduleDate"]).ToString("MM/dd/yyyy") : "") + "',";
                    pCareGiver += dr["Score"] + ",";
                }
            }
            #endregion

            #region Family Empowerment Scale
            if (dr["SurveyID"] != DBNull.Value && (int)dr["SurveyID"] == 14)
            {
                if (dr["UserType"] != DBNull.Value && (int)dr["UserType"] == 1)
                {
                    dPES += "'" + (dr["ScheduleDate"] != DBNull.Value ? Convert.ToDateTime(dr["ScheduleDate"]).ToString("MM/dd/yyyy") : "") + "',";
                    pPES += dr["Score"] + ",";
                }
            }
            #endregion

            #region GI Severity Index 
            if (dr["SurveyID"] != DBNull.Value && (int)dr["SurveyID"] == 3)
            {
                if (dr["UserType"] != DBNull.Value && (int)dr["UserType"] == 1)
                {
                    dGSI += "'" + (dr["ScheduleDate"] != DBNull.Value ? Convert.ToDateTime(dr["ScheduleDate"]).ToString("MM/dd/yyyy") : "") + "',";
                    pGSI += dr["Score"] + ",";
                }
            }
            #endregion

            #region Academic Progress Rating Scales
            if (dr["SurveyID"] != DBNull.Value && (int)dr["SurveyID"] == 27)
            {
                if (dr["UserType"] != DBNull.Value && (int)dr["UserType"] == 2)
                {
                    dAPRS += "'" + (dr["ScheduleDate"] != DBNull.Value ? Convert.ToDateTime(dr["ScheduleDate"]).ToString("MM/dd/yyyy") : "") + "',";
                    tAPRS += dr["Score"] + ",";
                }
            }
            #endregion

            #region Weekly Teacher
            if (dr["SurveyID"] != DBNull.Value && (int)dr["SurveyID"] == 24)
            {
                if (dr["UserType"] != DBNull.Value && (int)dr["UserType"] == 2)
                {
                    dWT += "'" + (dr["ScheduleDate"] != DBNull.Value ? Convert.ToDateTime(dr["ScheduleDate"]).ToString("MM/dd/yyyy") : "") + "',";
                    tWT += dr["Score"] + ",";
                }
            }
            #endregion
        }
    }

    if (dsCharts != null && dsCharts.Tables != null && dsCharts.Tables.Count > 0 && dsCharts.Tables[1].Rows.Count > 0)
    {
        foreach (DataRow dr in dsCharts.Tables[1].Rows)
        {
            #region Aberrant  Behaviour Checklist : Parent Quarterly Trend
            if (dr["SurveyID"] != DBNull.Value /*&& (int)dr["SurveyID"] == 4*/)
            {
                if (dr["UserType"] != DBNull.Value && (int)dr["UserType"] == 1)
                {
                    dpABCg += "'" + (dr["ScheduleDate"] != DBNull.Value ? Convert.ToDateTime(dr["ScheduleDate"]).ToString("MM/dd/yyyy") : "") + "',";
                    if(dr["QuestionGroup"].ToString() == "Auditory Sensitivity")
                    {
                        aspABCg += dr["Score"] + ",";
                    }
                    else
                    {
                        aspABCg += "0" + ",";
                    }

                    if (dr["QuestionGroup"].ToString() == "Taste/Smell Sensitivity")
                    {
                        tstpABCg += dr["Score"] + ",";
                    }
                    else
                    {
                        tstpABCg += "0" + ",";
                    }

                    if (dr["QuestionGroup"].ToString() == "Underresponsiveness")
                    {
                        unpABCg += dr["Score"] + ",";
                    }
                    else
                    {
                        unpABCg += "0" + ",";
                    }

                    if (dr["QuestionGroup"].ToString() == "Visual Sensitivity")
                    {
                        vspABCg += dr["Score"] + ",";
                    }
                    else
                    {
                        vspABCg += "0" + ",";
                    }

                    if (dr["QuestionGroup"].ToString() == "Questionnaire")
                    {
                        qpABCg += dr["Score"] + ",";
                    }
                    else
                    {
                        qpABCg += "0" + ",";
                    }
                }
            }
            #endregion

            #region Aberrant  Behaviour Checklist : Teacher Quarterly Trend
            if (dr["SurveyID"] != DBNull.Value /*&& (int)dr["SurveyID"] == 4*/)
            {
                if (dr["UserType"] != DBNull.Value && (int)dr["UserType"] == 2)
                {
                    dtABCg += "'" + (dr["ScheduleDate"] != DBNull.Value ? Convert.ToDateTime(dr["ScheduleDate"]).ToString("MM/dd/yyyy") : "") + "',";
                    if (dr["QuestionGroup"].ToString() == "Auditory Sensitivity")
                    {
                        astABCg += dr["Score"] + ",";
                    }
                    else
                    {
                        astABCg += "0" + ",";
                    }

                    if (dr["QuestionGroup"].ToString() == "Taste/Smell Sensitivity")
                    {
                        tsttABCg += dr["Score"] + ",";
                    }
                    else
                    {
                        tsttABCg += "0" + ",";
                    }

                    if (dr["QuestionGroup"].ToString() == "Underresponsiveness")
                    {
                        untABCg += dr["Score"] + ",";
                    }
                    else
                    {
                        untABCg += "0" + ",";
                    }

                    if (dr["QuestionGroup"].ToString() == "Visual Sensitivity")
                    {
                        vstABCg += dr["Score"] + ",";
                    }
                    else
                    {
                        vstABCg += "0" + ",";
                    }

                    if (dr["QuestionGroup"].ToString() == "Questionnaire")
                    {
                        qtABCg += dr["Score"] + ",";
                    }
                    else
                    {
                        qtABCg += "0" + ",";
                    }
                }
            }
            #endregion

            #region Social Responsivness Scale(SRS) sub scale : Parent Quarterly Trend
            if (dr["SurveyID"] != DBNull.Value /*&& (int)dr["SurveyID"] == 4*/)
            {
                if (dr["UserType"] != DBNull.Value && (int)dr["UserType"] == 1)
                {
                    dpSRSg += "'" + (dr["ScheduleDate"] != DBNull.Value ? Convert.ToDateTime(dr["ScheduleDate"]).ToString("MM/dd/yyyy") : "") + "',";
                    if (dr["QuestionGroup"].ToString() == "Auditory Sensitivity")
                    {
                        aspSRSg += dr["Score"] + ",";
                    }
                    else
                    {
                        aspSRSg += "0" + ",";
                    }

                    if (dr["QuestionGroup"].ToString() == "Taste/Smell Sensitivity")
                    {
                        tstpSRSg += dr["Score"] + ",";
                    }
                    else
                    {
                        tstpSRSg += "0" + ",";
                    }

                    if (dr["QuestionGroup"].ToString() == "Underresponsiveness")
                    {
                        unpSRSg += dr["Score"] + ",";
                    }
                    else
                    {
                        unpSRSg += "0" + ",";
                    }

                    if (dr["QuestionGroup"].ToString() == "Visual Sensitivity")
                    {
                        vspSRSg += dr["Score"] + ",";
                    }
                    else
                    {
                        vspSRSg += "0" + ",";
                    }

                    if (dr["QuestionGroup"].ToString() == "Questionnaire")
                    {
                        qpSRSg += dr["Score"] + ",";
                    }
                    else
                    {
                        qpSRSg += "0" + ",";
                    }
                }
            }
            #endregion

            #region Social Responsivness Scale(SRS) sub scale : Teacher Quarterly Trend
            if (dr["SurveyID"] != DBNull.Value /*&& (int)dr["SurveyID"] == 4*/)
            {
                if (dr["UserType"] != DBNull.Value && (int)dr["UserType"] == 2)
                {
                    dtSRSg += "'" + (dr["ScheduleDate"] != DBNull.Value ? Convert.ToDateTime(dr["ScheduleDate"]).ToString("MM/dd/yyyy") : "") + "',";
                    if (dr["QuestionGroup"].ToString() == "Auditory Sensitivity")
                    {
                        astSRSg += dr["Score"] + ",";
                    }
                    else
                    {
                        astSRSg += "0" + ",";
                    }

                    if (dr["QuestionGroup"].ToString() == "Taste/Smell Sensitivity")
                    {
                        tsttSRSg += dr["Score"] + ",";
                    }
                    else
                    {
                        tsttSRSg += "0" + ",";
                    }

                    if (dr["QuestionGroup"].ToString() == "Underresponsiveness")
                    {
                        untSRSg += dr["Score"] + ",";
                    }
                    else
                    {
                        untSRSg += "0" + ",";
                    }

                    if (dr["QuestionGroup"].ToString() == "Visual Sensitivity")
                    {
                        vstSRSg += dr["Score"] + ",";
                    }
                    else
                    {
                        vstSRSg += "0" + ",";
                    }

                    if (dr["QuestionGroup"].ToString() == "Questionnaire")
                    {
                        qtSRSg += dr["Score"] + ",";
                    }
                    else
                    {
                        qtSRSg += "0" + ",";
                    }
                }
            }
            #endregion

            #region PedsQL Parent Report : Parent Quarterly Trend
            if (dr["SurveyID"] != DBNull.Value /*&& (int)dr["SurveyID"] == 4*/)
            {
                if (dr["UserType"] != DBNull.Value && (int)dr["UserType"] == 1)
                {
                    dpPQLg += "'" + (dr["ScheduleDate"] != DBNull.Value ? Convert.ToDateTime(dr["ScheduleDate"]).ToString("MM/dd/yyyy") : "") + "',";
                    if (dr["QuestionGroup"].ToString() == "Auditory Sensitivity")
                    {
                        aspPQLg += dr["Score"] + ",";
                    }
                    else
                    {
                        aspPQLg += "0" + ",";
                    }

                    if (dr["QuestionGroup"].ToString() == "Taste/Smell Sensitivity")
                    {
                        tstpPQLg += dr["Score"] + ",";
                    }
                    else
                    {
                        tstpPQLg += "0" + ",";
                    }
                }
            }
            #endregion

            #region * When level changes for Child
            if (dr["SurveyID"] != DBNull.Value /*&& (int)dr["SurveyID"] == 4*/)
            {
                if (dr["UserType"] != DBNull.Value/* && (int)dr["UserType"] == 1*/)
                {
                    dpWLCg += "'" + (dr["ScheduleDate"] != DBNull.Value ? Convert.ToDateTime(dr["ScheduleDate"]).ToString("MM/dd/yyyy") : "") + "',";
                    if (dr["QuestionGroup"].ToString() == "Auditory Sensitivity")
                    {
                        aspWLCg += dr["Score"] + ",";
                    }
                    else
                    {
                        aspWLCg += "0" + ",";
                    }

                    if (dr["QuestionGroup"].ToString() == "Taste/Smell Sensitivity")
                    {
                        tstpWLCg += dr["Score"] + ",";
                    }
                    else
                    {
                        tstpWLCg += "0" + ",";
                    }
                }
            }
            #endregion
        }
    }
}


