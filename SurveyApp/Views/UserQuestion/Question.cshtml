                            savequestions(null, null, null, childId, SurID, "SubmitStatus")
                        }
                        $("#success-alert").fadeTo(2000, 500).slideUp(500, function () {
                            $(this).show();
                            $("#success-alert").alert('close');
                            history.back();
                        });

                    }
                });
            }


        });


