                }
                noQ++;
            });

            var prcnt = (Math.round((nofQ / noQ) * 100).toFixed(0)) + "%";

            var url = "/UserQuestion/SaveUserQuestion";
            var period = "@(String.IsNullOrEmpty(Request.QueryString["perioddate"]) == true ? "" : Request.QueryString["perioddate"].ToString())";

        $.post(url, { QuestionID: qid, AnswerID: ansid, score: s, childid: cid, SurveyID: SurID, statusparm: status, percentage : prcnt, scheduleDate : period }, function (data) {

        });
